  "tokenColors": [
    {
      "scope": ["strong"],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["header"],
      "settings": {
        "foreground": "#78D1E1"
      }
    },
    {
      "scope": ["source"],
      "settings": {
        "foreground": "#d4d4d4"
      }
    },
    {
      "scope": ["meta.diff", "meta.diff.header"],
      "settings": {
        "foreground": "#624051"
      }
    },
    {
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#5dd465"
      }
    },
    {
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#e96389"
      }
    },
    {
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#E89E64"
      }
    },
    {
      "scope": ["invalid"],
      "settings": {
        "foreground": "#E96379",
        "fontStyle": "underline"
      }
    },
    {
      "scope": ["invalid.deprecated"],
      "settings": {
        "foreground": "#d4d4d4",
        "fontStyle": "underline"
      }
